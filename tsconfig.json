{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~knex/*": ["src/knex/*"],
      "~lib/*": ["src/lib/*"],
      "~root/*": ["src/*"],
      "~services/*": ["src/services/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "commonjs",
    "outDir": "build",
    "rootDir": "src",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES6",
    "typeRoots": ["./node_modules/@types", "./src/**/*.ts", "./custom.d.ts"]
  },
  "exclude": ["node_modules"]
}
